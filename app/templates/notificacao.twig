{% if messages.error %}
    <div class="ui error message">
        <i class="close icon"></i>
        <div class="header">Erro</div>
        <ul class="list">
            {% for item in messages.error %}
                <li>{{item}}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}


{% if messages.success %}
    <div class="ui success message">
        <i class="close icon"></i>
        <div class="header">Sucesso</div>
        <ul class="list">
            {% for item in messages.success %}
                <li>{{item}}</li>
            {% endfor %}
        </ul>
    </div>
{% endif %}